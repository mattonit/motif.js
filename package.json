{
  "name": "snakepit",
  "version": "0.0.1",
  "description": "Snakepit UI Framework for React",
  "main": "dist/snakepit.js",
  "module": "dist/snakepit.esm.js",
  "scripts": {
    "build": "npm run build:js && npm run build:scss",
    "build:docs": "webpack --config config/webpack.docs.config.js --progress --colors",
    "build:js": "rollup -c",
    "build:scss": "node-sass src/scss -o dist/",
    "dev": "webpack-dev-server --config config/webpack.docs.config.js --progress --colors",
    "lint": "npm run lint:js && npm run lint:scss && npm run lint:scss:docs",
    "lint:js": "eslint src/**/*.js",
    "lint:scss": "stylelint --syntax scss src/scss/**/*.scss",
    "lint:scss:docs": "stylelint --syntax scss src/docs/scss/**/*.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattonit/snakepit.git"
  },
  "author": "mattonit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattonit/snakepit/issues"
  },
  "homepage": "https://github.com/mattonit/snakepit#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "rollup": "^2.32.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-replace": "^2.2.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}
